{
    "extends": [
        "@typescript-eslint/recommended",
        "airbnb-typescript",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "warn",
        "@typescript-eslint/no-unused-vars": "error",
        "prefer-const": "error"
    },
    "env": {
        "node": true,
        "jest": true,
        "es2021": true
    }
}